<?xml version="1.0" encoding="UTF-8"?>
<celerio xmlns="http://www.jaxio.com/schema/celerio" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.jaxio.com/schema/celerio ./celerio.xsd">
    <configuration rootPackage="br.com.framework.piloto" applicationName="pilotojee7" jpaUseCatalog="FALSE" jpaUseSchema="TRUE">
        <!-- Which tables are used for generation -->
        <tables>
            <!-- <table include="true" pattern="*" /> -->
            <table include="false" pattern="flyway_schema_history"/>
        </tables>
        
        <!-- default global sequence mapping (optional). initialValue can be set and defaults to 1 when is not set. -->
        <sequences>
            <sequence tableNamePattern="*" sequenceName="*" allocationSize="1" schema="cadastro" />
        </sequences>

        <!-- global number mapping (optional) -->
        <numberMappings>
            <numberMapping mappedType="M_BOOLEAN" columnSizeMin="1" columnSizeMax="2" columnDecimalDigitsMin="0" columnDecimalDigitsMax="0"/>
        </numberMappings>

        <!-- global date mapping (optional) -->
        <dateMappings>
            <dateMapping columnJdbcType="TIMESTAMP" mappedType="M_LOCALDATETIME"/>
            <dateMapping columnJdbcType="DATE" mappedType="M_LOCALDATE"/>
        </dateMappings>

        <!-- default configuration for fetch type / cascades / 2d level cache -->
        <defaultEntityCacheConfig usage="NONE"/>

        <defaultManyToOneConfig fetch="LAZY">
            <cascades>
                <cascade type="NONE"/>
            </cascades>
            <cacheConfig usage="NONE"/>
            <associationAction create="true" edit="true" view="true" select="true" autoComplete="true" remove="false"/>
        </defaultManyToOneConfig>

        <defaultOneToManyConfig orphanRemoval="true">
            <cascades>
                <cascade type="ALL"/>
            </cascades>
            <cacheConfig usage="NONE"/>
            <associationAction create="true" edit="true" view="true" select="true" autoComplete="true" remove="true"/>
        </defaultOneToManyConfig>

        <defaultOneToOneConfig fetch="LAZY">
            <cascades>
                <cascade type="NONE"/>
            </cascades>
            <cacheConfig usage="NONE"/>
            <associationAction create="true" edit="true" view="true" select="false" autoComplete="true" remove="false"/>
        </defaultOneToOneConfig>

        <defaultInverseOneToOneConfig fetch="LAZY">
            <cascades>
                <cascade type="ALL"/>
            </cascades>
            <cacheConfig usage="NONE"/>
            <associationAction create="false" edit="false" view="true" select="false" autoComplete="false"
                               remove="false"/>
        </defaultInverseOneToOneConfig>

        <defaultManyToManyConfig>
            <cascades>
                <cascade type="ALL"/>
            </cascades>
            <cacheConfig usage="NONE"/>
            <associationAction create="true" edit="true" view="true" select="true" autoComplete="false" remove="true"/>
        </defaultManyToManyConfig>

        <defaultInverseManyToManyConfig>
            <cascades>
                <cascade type="ALL"/>
            </cascades>
            <cacheConfig usage="NONE"/>
            <associationAction create="false" edit="true" view="true" select="false" autoComplete="false" remove="false"/>
        </defaultInverseManyToManyConfig>

        <conventions identifiableProperty="id"></conventions>
    </configuration>
    
    <!-- CHANGE IT OR COMMENT IT IF YOU USE YOUR OWN DATABASE -->

    <entityConfigs>

        <entityConfig tableName="assinatura" entityName="Assinatura" sequenceName="seq_assinatura" schema="cadastro" label="Assinatura">
            <columnConfigs>
            
	            <columnConfig columnName="id_assinatura" formField="false" fieldName="id" label="Id"/>
	            <columnConfig columnName="id_usuario" fieldName="usuario" label="Usuário">
					<manyToOneConfig var="usuario" />
				</columnConfig>
				<columnConfig columnName="id_plano" fieldName="plano" label="Plano">
					<manyToOneConfig var="plano" />
				</columnConfig>
	            <columnConfig columnName="data_inicial" fieldName="dataInicial" searchResult="true" displayOrder="1" label="Data inicial"/>
                <columnConfig columnName="data_final" fieldName="dataFinal" searchResult="true" displayOrder="2" label="Data final"/>
                
				<columnConfig columnName="version" fieldName="version" formField="false" searchField="false" version="true" type="BIGINT" />
            	
            	<columnConfig columnName="creation_date" fieldName="creationDate" formField="false" searchField="false" searchResult="false" label="Criado em" />
            	<columnConfig columnName="creation_author" fieldName="creationAuthor" formField="false" searchField="false" searchResult="false" label="Criado por" />
                <columnConfig columnName="last_modification_date" fieldName="lastModificationDate" formField="false" searchField="false" searchResult="true" label="Modificado em" />
                <columnConfig columnName="last_modification_author" fieldName="lastModificationAuthor" formField="false" searchField="false" searchResult="false" label="Modificado por" />
                <columnConfig columnName="is_enabled" sharedEnumName="Status" fieldName="status" formField="false" searchField="false" searchResult="false" label="Status" />
                
            </columnConfigs>
        </entityConfig>
        
        <entityConfig tableName="ativo" entityName="Ativo" sequenceName="seq_ativo" schema="cadastro" label="Ativo">
            <columnConfigs>
            
	            <columnConfig columnName="id_ativo" formField="false" fieldName="id" label="Id"/>
	            <columnConfig columnName="sigla" fieldName="sigla" formField="true" unique="true" searchField="true" searchResult="true" displayOrder="1" label="Sigla"/>
                <columnConfig columnName="nome" fieldName="nome" formField="true" searchField="true" searchResult="true" displayOrder="2" label="Nome"/>
                <columnConfig columnName="descricao" fieldName="descricao" formField="true" searchField="false" searchResult="false" displayOrder="3" label="Descrição"/>
                <columnConfig columnName="categoria" fieldName="categoria" sharedEnumName="CategoriaAtivo" formField="true" searchField="false" searchResult="false" displayOrder="4" label="Categoria"/>
                <columnConfig columnName="ultima_cotacao" fieldName="ultimaCotacao" formField="true" searchField="false" searchResult="true" displayOrder="5" label="Última cotação"/>
                <columnConfig columnName="data_ultima_cotacao" fieldName="dataUltimaCotacao" formField="true" searchField="false" searchResult="true" displayOrder="6" label="Última cotação"/>
                
				<columnConfig columnName="version" fieldName="version" formField="false" searchField="false" version="true" type="BIGINT" />
            	
            	<columnConfig columnName="creation_date" fieldName="creationDate" formField="false" searchField="false" searchResult="false" label="Criado em" />
            	<columnConfig columnName="creation_author" fieldName="creationAuthor" formField="false" searchField="false" searchResult="false" label="Criado por" />
                <columnConfig columnName="last_modification_date" fieldName="lastModificationDate" formField="false" searchField="false" searchResult="true" label="Modificado em" />
                <columnConfig columnName="last_modification_author" fieldName="lastModificationAuthor" formField="false" searchField="false" searchResult="false" label="Modificado por" />
                <columnConfig columnName="is_enabled" sharedEnumName="Status" fieldName="status" formField="false" searchField="false" searchResult="false" label="Status" />
                
            </columnConfigs>
        </entityConfig>
        
        <entityConfig tableName="carteira" entityName="Carteira" sequenceName="seq_carteira" schema="cadastro" label="Carteira">
            <columnConfigs>
            
	            <columnConfig columnName="id_carteira" formField="false" fieldName="id" label="Id"/>
	            <columnConfig columnName="id_usuario" fieldName="usuario" label="Usuário" displayOrder="1">
					<manyToOneConfig var="usuario" />
				</columnConfig>
	            <columnConfig columnName="apelido" fieldName="apelido" formField="true" searchField="true" searchResult="true" displayOrder="2" label="Apelido"/>
                
				<columnConfig columnName="version" fieldName="version" formField="false" searchField="false" version="true" type="BIGINT" />
            	
            	<columnConfig columnName="creation_date" fieldName="creationDate" formField="false" searchField="false" searchResult="false" label="Criado em" />
            	<columnConfig columnName="creation_author" fieldName="creationAuthor" formField="false" searchField="false" searchResult="false" label="Criado por" />
                <columnConfig columnName="last_modification_date" fieldName="lastModificationDate" formField="false" searchField="false" searchResult="true" label="Modificado em" />
                <columnConfig columnName="last_modification_author" fieldName="lastModificationAuthor" formField="false" searchField="false" searchResult="false" label="Modificado por" />
                <columnConfig columnName="is_enabled" sharedEnumName="Status" fieldName="status" formField="false" searchField="false" searchResult="false" label="Status" />
                
            </columnConfigs>
        </entityConfig>
        
        <entityConfig tableName="compra" entityName="Compra" sequenceName="seq_compra" schema="cadastro" label="Compra">
            <columnConfigs>
            
	            <columnConfig columnName="id_compra" formField="false" fieldName="id" label="Id"/>
	            <columnConfig columnName="id_ativo" fieldName="ativo" label="Ativo" displayOrder="1">
					<manyToOneConfig var="ativo" />
				</columnConfig>
				<columnConfig columnName="id_carteira" fieldName="carteira" label="Carteira" displayOrder="2">
					<manyToOneConfig var="carteira" />
				</columnConfig>
	            <columnConfig columnName="preco_unitario" fieldName="precoUnitario" formField="true" searchField="false" searchResult="true" displayOrder="3" label="Preço Unitário"/>
	            <columnConfig columnName="quantidade" fieldName="quantidade" formField="true" searchField="false" searchResult="true" displayOrder="4" label="Quantidade"/>
	            <columnConfig columnName="valor_total" fieldName="valorTotal" formField="true" searchField="false" searchResult="true" displayOrder="5" label="Valor Total"/>
	            <columnConfig columnName="data_operacao" fieldName="dataOperacao" formField="true" searchField="false" searchResult="true" displayOrder="6" label="Data Operação"/>
                
				<columnConfig columnName="version" fieldName="version" formField="false" searchField="false" version="true" type="BIGINT" />
            	
            	<columnConfig columnName="creation_date" fieldName="creationDate" formField="false" searchField="false" searchResult="false" label="Criado em" />
            	<columnConfig columnName="creation_author" fieldName="creationAuthor" formField="false" searchField="false" searchResult="false" label="Criado por" />
                <columnConfig columnName="last_modification_date" fieldName="lastModificationDate" formField="false" searchField="false" searchResult="true" label="Modificado em" />
                <columnConfig columnName="last_modification_author" fieldName="lastModificationAuthor" formField="false" searchField="false" searchResult="false" label="Modificado por" />
                <columnConfig columnName="is_enabled" sharedEnumName="Status" fieldName="status" formField="false" searchField="false" searchResult="false" label="Status" />
                
            </columnConfigs>
        </entityConfig>
        
        <entityConfig tableName="venda" entityName="Venda" sequenceName="seq_venda" schema="cadastro" label="Venda">
            <columnConfigs>
            
	            <columnConfig columnName="id_venda" formField="false" fieldName="id" label="Id"/>
	            <columnConfig columnName="id_ativo" fieldName="ativo" formField="true" searchField="true" searchResult="true" displayOrder="1" label="Ativo" >
					<manyToOneConfig var="ativo" />
				</columnConfig>
				<columnConfig columnName="id_carteira" fieldName="carteira" formField="true" searchField="true" searchResult="true" displayOrder="2" label="Carteira">
					<manyToOneConfig var="carteira" />
				</columnConfig>
	            <columnConfig columnName="preco_unitario" fieldName="precoUnitario" formField="true" searchField="false" searchResult="true" displayOrder="3" label="Preço Unitário"/>
	            <columnConfig columnName="quantidade" fieldName="quantidade" formField="true" searchField="false" searchResult="true" displayOrder="4" label="Quantidade"/>
	            <columnConfig columnName="valor_total" fieldName="valorTotal" formField="true" searchField="false" searchResult="true" displayOrder="5" label="Valor Total"/>
	            <columnConfig columnName="data_operacao" fieldName="dataOperacao" formField="true" searchField="false" searchResult="true" displayOrder="6" label="Data Operação"/>
                
				<columnConfig columnName="version" fieldName="version" formField="false" searchField="false" version="true" type="BIGINT" />
            	
            	<columnConfig columnName="creation_date" fieldName="creationDate" formField="false" searchField="false" searchResult="false" label="Criado em" />
            	<columnConfig columnName="creation_author" fieldName="creationAuthor" formField="false" searchField="false" searchResult="false" label="Criado por" />
                <columnConfig columnName="last_modification_date" fieldName="lastModificationDate" formField="false" searchField="false" searchResult="true" label="Modificado em" />
                <columnConfig columnName="last_modification_author" fieldName="lastModificationAuthor" formField="false" searchField="false" searchResult="false" label="Modificado por" />
                <columnConfig columnName="is_enabled" sharedEnumName="Status" fieldName="status" formField="false" searchField="false" searchResult="false" label="Status" />
                
            </columnConfigs>
        </entityConfig>
        
        <entityConfig tableName="plano" entityName="Plano" sequenceName="seq_plano" schema="cadastro" label="Plano">
            <columnConfigs>
            
	            <columnConfig columnName="id_plano" formField="false" fieldName="id" label="Id"/>
	            <columnConfig columnName="nome" fieldName="nome" formField="true" searchField="true" searchResult="true" displayOrder="1" label="Nome"/>
	            <columnConfig columnName="descricao" fieldName="descricao" formField="true" searchField="false" searchResult="false" displayOrder="2" label="Descrição"/>
	            <columnConfig columnName="valor" fieldName="valor" formField="true" searchField="false" searchResult="true" displayOrder="3" label="Valor"/>
	            <columnConfig columnName="vigencia_dias" fieldName="vigenciaDias" formField="true" searchField="false" searchResult="true" displayOrder="4" label="Vigência (dias)"/>
                
				<columnConfig columnName="version" fieldName="version" formField="false" searchField="false" version="true" type="BIGINT" />
            	
            	<columnConfig columnName="creation_date" fieldName="creationDate" formField="false" searchField="false" searchResult="false" label="Criado em" />
            	<columnConfig columnName="creation_author" fieldName="creationAuthor" formField="false" searchField="false" searchResult="false" label="Criado por" />
                <columnConfig columnName="last_modification_date" fieldName="lastModificationDate" formField="false" searchField="false" searchResult="true" label="Modificado em" />
                <columnConfig columnName="last_modification_author" fieldName="lastModificationAuthor" formField="false" searchField="false" searchResult="false" label="Modificado por" />
                <columnConfig columnName="is_enabled" sharedEnumName="Status" fieldName="status" formField="false" searchField="false" searchResult="false" label="Status" />
                
            </columnConfigs>
        </entityConfig>
        
        <entityConfig tableName="recomendacao" entityName="Recomendacao" sequenceName="seq_recomendacao" schema="cadastro" label="Recomendação">
            <columnConfigs>
            
	            <columnConfig columnName="id_recomendacao" formField="false" fieldName="id" label="Id"/>
	            <columnConfig columnName="titulo" fieldName="titulo" formField="true" searchField="true" searchResult="true" displayOrder="1" label="Título" />
	            <columnConfig columnName="texto" fieldName="texto" formField="true" searchField="false" searchResult="false" displayOrder="2" label="Texto" />
	            <columnConfig columnName="id_ativo" fieldName="ativo" formField="true" searchField="true" searchResult="true" displayOrder="3" label="Ativo" >
					<manyToOneConfig var="ativo" />
				</columnConfig>
	            <columnConfig columnName="preco_entrada" fieldName="precoEntrada" formField="true" searchField="false" searchResult="false" displayOrder="4" label="Preço Entrada"/>
	            <columnConfig columnName="preco_saida" fieldName="precoSaida" formField="true" searchField="false" searchResult="false" displayOrder="5" label="Preço Saída"/>
	            <columnConfig columnName="preco_saida_parcial" fieldName="precoSaidaParcial" formField="true" searchField="false" searchResult="false" displayOrder="5" label="Preço Saída Parcial"/>
                
				<columnConfig columnName="version" fieldName="version" formField="false" searchField="false" version="true" type="BIGINT" />
            	
            	<columnConfig columnName="creation_date" fieldName="creationDate" formField="false" searchField="false" searchResult="false" label="Criado em" />
            	<columnConfig columnName="creation_author" fieldName="creationAuthor" formField="false" searchField="false" searchResult="false" label="Criado por" />
                <columnConfig columnName="last_modification_date" fieldName="lastModificationDate" formField="false" searchField="false" searchResult="true" label="Modificado em" />
                <columnConfig columnName="last_modification_author" fieldName="lastModificationAuthor" formField="false" searchField="false" searchResult="false" label="Modificado por" />
                <columnConfig columnName="is_enabled" sharedEnumName="Status" fieldName="status" formField="false" searchField="false" searchResult="false" label="Status" />
                
            </columnConfigs>
        </entityConfig>
        
        <entityConfig tableName="usuario" entityName="Usuario" sequenceName="seq_usuario" schema="cadastro" label="Usuário">
            <columnConfigs>
            
	            <columnConfig columnName="id_usuario" formField="false" fieldName="id" label="Id"/>
	            <columnConfig columnName="login" fieldName="login" formField="true" searchField="true" searchResult="true" displayOrder="1" label="Login" />
	            <columnConfig columnName="email" fieldName="email" formField="true" searchField="true" searchResult="true" displayOrder="2" label="E-mail" />
	            <columnConfig columnName="telefone" fieldName="telefone" formField="true" searchField="true" searchResult="true" displayOrder="3" label="Telefone"/>
                
				<columnConfig columnName="version" fieldName="version" formField="false" searchField="false" version="true" type="BIGINT" />
            	
            	<columnConfig columnName="creation_date" fieldName="creationDate" formField="false" searchField="false" searchResult="false" label="Criado em" />
            	<columnConfig columnName="creation_author" fieldName="creationAuthor" formField="false" searchField="false" searchResult="false" label="Criado por" />
                <columnConfig columnName="last_modification_date" fieldName="lastModificationDate" formField="false" searchField="false" searchResult="true" label="Modificado em" />
                <columnConfig columnName="last_modification_author" fieldName="lastModificationAuthor" formField="false" searchField="false" searchResult="false" label="Modificado por" />
                <columnConfig columnName="is_enabled" sharedEnumName="Status" fieldName="status" formField="false" searchField="false" searchResult="false" label="Status" />
                
            </columnConfigs>
        </entityConfig>
        
    </entityConfigs>
    
    <!-- CHANGE IT OR COMMENT IT IF YOU USE YOUR OWN DATABASE -->
    <!-- Generate enums used by the entities above -->
    <sharedEnumConfigs>
        <sharedEnumConfig name="Status" type="ORDINAL">
            <enumValues>
                <enumValue value="INACTIVE" label="Inativo" />
                <enumValue value="ACTIVE" label="Ativo" />
            </enumValues>
        </sharedEnumConfig>
        <sharedEnumConfig name="SimNao" type="STRING">
            <enumValues>
                <enumValue value="S" label="Sim" />
                <enumValue value="N" label="Não" />
            </enumValues>
        </sharedEnumConfig>
        <sharedEnumConfig name="CategoriaAtivo" type="STRING">
            <enumValues>
                <enumValue value="AC" label="Ações" />
            </enumValues>
        </sharedEnumConfig>
    </sharedEnumConfigs>

</celerio>