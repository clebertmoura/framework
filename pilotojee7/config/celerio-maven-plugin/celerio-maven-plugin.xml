<?xml version="1.0" encoding="UTF-8"?>
<celerio xmlns="http://www.jaxio.com/schema/celerio" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.jaxio.com/schema/celerio ./celerio.xsd">
    <configuration rootPackage="br.com.framework.piloto" applicationName="pilotojee7" jpaUseCatalog="FALSE" jpaUseSchema="TRUE">
        <!-- Which tables are used for generation -->
        <tables>
            <!-- <table include="true" pattern="*" /> -->
            <table include="false" pattern="flyway_schema_history"/>
        </tables>
        
        <!-- default global sequence mapping (optional). initialValue can be set and defaults to 1 when is not set. -->
        <sequences>
            <sequence tableNamePattern="*" sequenceName="*" allocationSize="1" schema="cadastro" />
        </sequences>

        <!-- global number mapping (optional) -->
        <numberMappings>
            <numberMapping mappedType="M_BOOLEAN" columnSizeMin="1" columnSizeMax="2" columnDecimalDigitsMin="0" columnDecimalDigitsMax="0"/>
        </numberMappings>

        <!-- global date mapping (optional) -->
        <dateMappings>
            <dateMapping columnJdbcType="TIMESTAMP" mappedType="M_LOCALDATETIME"/>
            <dateMapping columnJdbcType="DATE" mappedType="M_LOCALDATE"/>
        </dateMappings>

        <!-- default configuration for fetch type / cascades / 2d level cache -->
        <defaultEntityCacheConfig usage="NONE"/>

        <defaultManyToOneConfig fetch="LAZY">
            <cascades>
                <cascade type="NONE"/>
            </cascades>
            <cacheConfig usage="NONE"/>
            <associationAction create="true" edit="true" view="true" select="true" autoComplete="true" remove="false"/>
        </defaultManyToOneConfig>

        <defaultOneToManyConfig orphanRemoval="true">
            <cascades>
                <cascade type="ALL"/>
            </cascades>
            <cacheConfig usage="NONE"/>
            <associationAction create="true" edit="true" view="true" select="true" autoComplete="true" remove="true"/>
        </defaultOneToManyConfig>

        <defaultOneToOneConfig fetch="LAZY">
            <cascades>
                <cascade type="NONE"/>
            </cascades>
            <cacheConfig usage="NONE"/>
            <associationAction create="true" edit="true" view="true" select="false" autoComplete="true" remove="false"/>
        </defaultOneToOneConfig>

        <defaultInverseOneToOneConfig fetch="LAZY">
            <cascades>
                <cascade type="ALL"/>
            </cascades>
            <cacheConfig usage="NONE"/>
            <associationAction create="false" edit="false" view="true" select="false" autoComplete="false"
                               remove="false"/>
        </defaultInverseOneToOneConfig>

        <defaultManyToManyConfig>
            <cascades>
                <cascade type="ALL"/>
            </cascades>
            <cacheConfig usage="NONE"/>
            <associationAction create="true" edit="true" view="true" select="true" autoComplete="false" remove="true"/>
        </defaultManyToManyConfig>

        <defaultInverseManyToManyConfig>
            <cascades>
                <cascade type="ALL"/>
            </cascades>
            <cacheConfig usage="NONE"/>
            <associationAction create="false" edit="true" view="true" select="false" autoComplete="false" remove="false"/>
        </defaultInverseManyToManyConfig>

        <conventions identifiableProperty="id"></conventions>
    </configuration>
    
    <!-- CHANGE IT OR COMMENT IT IF YOU USE YOUR OWN DATABASE -->

    <entityConfigs>

        <entityConfig tableName="driver" entityName="Driver" sequenceName="seq_driver" schema="cadastro">
            <columnConfigs>
            
	            <columnConfig columnName="id_driver" formField="false" fieldName="id" />
	            <columnConfig columnName="name" fieldName="name" searchField="true" searchResult="true" displayOrder="1"/>
                <columnConfig columnName="cpf" fieldName="cpf" searchField="true" searchResult="true" displayOrder="2"/>
                <columnConfig columnName="driver_key" fieldName="driverKey" searchField="false" searchResult="false" displayOrder="3"/>
                
				<columnConfig columnName="version" fieldName="version" formField="false" searchField="false" version="true" type="BIGINT" />
            	
            	<columnConfig columnName="creation_date" fieldName="creationDate" formField="false" searchField="false" searchResult="false" />
            	<columnConfig columnName="creation_author" fieldName="creationAuthor" formField="false" searchField="false" searchResult="false" />
                <columnConfig columnName="last_modification_date" fieldName="lastModificationDate" formField="false" searchField="false" searchResult="true" />
                <columnConfig columnName="last_modification_author" fieldName="lastModificationAuthor" formField="false" searchField="false" searchResult="false" />
                <columnConfig columnName="fl_enabled" sharedEnumName="Status" fieldName="status" formField="false" searchField="false" searchResult="false" />
                
            </columnConfigs>
        </entityConfig>
        
        <entityConfig tableName="device" entityName="Device" sequenceName="seq_device" schema="cadastro">
            <columnConfigs>
            
	            <columnConfig columnName="id_device" formField="false" fieldName="id" />
	            <columnConfig columnName="id_driver">
					<manyToOneConfig var="driver"/>
				</columnConfig>
	            
	            <columnConfig columnName="uuid" fieldName="uuid" searchField="false" searchResult="false" displayOrder="1"/>
                <columnConfig columnName="model" fieldName="model" searchField="true" searchResult="true" displayOrder="2"/>
                <columnConfig columnName="platform" fieldName="platform" searchField="true" searchResult="true" displayOrder="3"/>
                <columnConfig columnName="device_version" fieldName="deviceVersion" searchField="false" searchResult="false" displayOrder="4"/>
                <columnConfig columnName="mobile_number" fieldName="mobileNumber" searchField="true" searchResult="true" displayOrder="5"/>
                <columnConfig columnName="fl_mobnumber_verified" fieldName="mobileNumberVerified" sharedEnumName="SimNao" searchField="true" searchResult="true" displayOrder="6"/>
                <columnConfig columnName="pin_verification" fieldName="pinVerification" searchField="true" searchResult="false" displayOrder="7"/>
                
				<columnConfig columnName="version" fieldName="version" formField="false" searchField="false" version="true" type="BIGINT" />
            	
            	<columnConfig columnName="creation_date" fieldName="creationDate" formField="false" searchField="false" searchResult="false" />
            	<columnConfig columnName="creation_author" fieldName="creationAuthor" formField="false" searchField="false" searchResult="false" />
                <columnConfig columnName="last_modification_date" fieldName="lastModificationDate" formField="false" searchField="false" searchResult="true" />
                <columnConfig columnName="last_modification_author" fieldName="lastModificationAuthor" formField="false" searchField="false" searchResult="false" />
                <columnConfig columnName="fl_enabled" sharedEnumName="Status" fieldName="status" formField="false" searchField="false" searchResult="false" />
                
            </columnConfigs>
        </entityConfig>
        
        <entityConfig tableName="access_log" entityName="AccessLog" sequenceName="seq_access_log" schema="cadastro">
            <columnConfigs>
            
	            <columnConfig columnName="id_access_log" formField="false" fieldName="id" />
	            <columnConfig columnName="id_driver">
					<manyToOneConfig var="driver"/>
				</columnConfig>
	            <columnConfig columnName="id_device">
					<manyToOneConfig var="device"/>
				</columnConfig>
	            
	            <columnConfig columnName="access_date" fieldName="accesDate" searchField="false" searchResult="false" displayOrder="1"/>
                <columnConfig columnName="api_url" fieldName="apiUrl" searchField="true" searchResult="true" displayOrder="2"/>
                <columnConfig columnName="api_request_data" fieldName="apiRequestData" searchField="false" searchResult="false" displayOrder="3"/>
                
				<columnConfig columnName="version" fieldName="version" formField="false" searchField="false" version="true" type="BIGINT" />
                
            </columnConfigs>
        </entityConfig>
        
        <entityConfig tableName="station" entityName="Station" sequenceName="seq_station" schema="cadastro">
            <columnConfigs>
            
	            <columnConfig columnName="id_station" formField="false" fieldName="id" />
	            
	            <columnConfig columnName="name" fieldName="name" searchField="true" searchResult="true" displayOrder="1"/>
                <columnConfig columnName="company_name" fieldName="companyName" formField="true" searchField="true" searchResult="true" displayOrder="2"/>
                <columnConfig columnName="cnpj" fieldName="cnpj" formField="true" searchField="true" searchResult="true" displayOrder="3"/>
                <columnConfig columnName="ie" fieldName="ie" formField="true" searchField="true" searchResult="true" displayOrder="4"/>
                <columnConfig columnName="phones" fieldName="phones" formField="true" searchField="true" searchResult="true" displayOrder="5"/>
                <columnConfig columnName="address" fieldName="address" formField="true" searchField="false" searchResult="false" displayOrder="6"/>
                <columnConfig columnName="zipcode" fieldName="zipcode" formField="true" searchField="true" searchResult="false" displayOrder="7"/>
                <columnConfig columnName="city" fieldName="city" formField="true" searchField="true" searchResult="true" displayOrder="8"/>
                <columnConfig columnName="state" fieldName="state" formField="true" searchField="true" searchResult="true" displayOrder="9"/>
                <columnConfig columnName="lat" fieldName="lat" formField="true" searchField="false" searchResult="false" displayOrder="10"/>
                <columnConfig columnName="lng" fieldName="lat" formField="true" searchField="false" searchResult="false" displayOrder="11"/>
                
				<columnConfig columnName="version" fieldName="version" formField="false" searchField="false" version="true" type="BIGINT" />
            	
            	<columnConfig columnName="creation_date" fieldName="creationDate" formField="true" searchField="false" searchResult="false" />
            	<columnConfig columnName="creation_author" fieldName="creationAuthor" formField="false" searchField="false" searchResult="false" />
                <columnConfig columnName="last_modification_date" fieldName="lastModificationDate" formField="false" searchField="false" searchResult="false" />
                <columnConfig columnName="last_modification_author" fieldName="lastModificationAuthor" formField="false" searchField="false" searchResult="false" />
                <columnConfig columnName="fl_enabled" sharedEnumName="Status" fieldName="status" formField="false" searchField="false" searchResult="false" />
                
            </columnConfigs>
        </entityConfig>
        
    </entityConfigs>
    
    <!-- CHANGE IT OR COMMENT IT IF YOU USE YOUR OWN DATABASE -->
    <!-- Generate enums used by the entities above -->
    <sharedEnumConfigs>
        <sharedEnumConfig userType="br.com.framework.domain.enums.Status" name="Status" type="ORDINAL">
            <enumValues>
                <enumValue value="INACTIVE" label="Inativo" />
                <enumValue value="ACTIVE" label="Ativo" />
            </enumValues>
        </sharedEnumConfig>
        <sharedEnumConfig name="SimNao" type="STRING">
            <enumValues>
                <enumValue value="S" label="Sim" />
                <enumValue value="N" label="Não" />
            </enumValues>
        </sharedEnumConfig>
    </sharedEnumConfigs>

</celerio>